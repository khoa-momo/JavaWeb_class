<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ThirdServlet</servlet-name>
        <servlet-class>Controller.ThirdServlet</servlet-class>
        <init-param>
            <param-name>Name</param-name>
            <param-value>Value</param-value>
        </init-param>
        <init-param>
            <param-name>Course</param-name>
            <param-value>Value</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>FourthServlet</servlet-name>
        <servlet-class>Controller.FourthServlet</servlet-class>
        <init-param>
            <param-name>Name</param-name>
            <param-value>NoName</param-value>
        </init-param>
        <init-param>
            <param-name>Course</param-name>
            <param-value>NoCourse</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>Calculator_test</servlet-name>
        <servlet-class>Controller.Calculator_test</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ThirdServlet</servlet-name>
        <url-pattern>/ThirdServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FourthServlet</servlet-name>
        <url-pattern>/Fourth</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Calculator_test</servlet-name>
        <url-pattern>/Calculator_test</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
